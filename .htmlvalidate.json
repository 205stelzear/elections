{
  "elements": [
    "html5",
    {
      "img": {
        "attributes": {
          "src": [
            "/.+/",
            ""
          ]
        }
      }
    }
  ],
  "extends": [
    "html-validate:recommended"
  ],
  "rules": {
    "no-trailing-whitespace": "off",
    "no-inline-style": "off",
    "wcag/h37": "off",
    "wcag/h32": "off"
  }
}